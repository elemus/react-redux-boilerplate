{
  "name": "react-redux-todo-example",
  "version": "0.0.0",
  "description": "React-Redux Todo Example",
  "author": "Sergey Atyagin",
  "license": "MIT",
  "homepage": "https://github.com/elemus/react-redux-todo-example",
  "keywords": [
    "react",
    "react-router",
    "redux",
    "webpack",
    "hot",
    "reload",
    "hmr",
    "boilerplate"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:elemus/react-redux-todo-example.git"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack -p --config ./webpack/config.prod.js --progress --profile --colors --bail",
    "clean": "rimraf dist coverage",
    "coverage": "nyc npm test",
    "lint": "eslint src && sass-lint src/styles/**/*.scss -v -q",
    "start": "webpack-dev-server --config ./webpack/config.dev.js --hot",
    "test": "cross-env NODE_ENV=test mocha --opts ./mocha.opts --recursive"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "include": [
      "src/**/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "check-coverage": true,
    "cache": true
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "bootstrap": "^4.0.0-alpha.6",
    "classnames": "^2.2.5",
    "history": "^4.6.0",
    "prop-types": "^15.5.10",
    "react": "^15.5.3",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.23.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.23.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.1",
    "enzyme": "^2.8.2",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "jsdom": "^10.1.0",
    "mocha": "^3.4.1",
    "node-sass": "^4.5.3",
    "nyc": "^10.3.2",
    "postcss-loader": "^2.0.5",
    "proxyquire": "^1.8.0",
    "react-addons-test-utils": "^15.5.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.5.4",
    "rimraf": "^2.6.1",
    "sass-lint": "^1.10.2",
    "sass-loader": "^6.0.5",
    "sinon": "^2.2.0",
    "sinon-chai": "^2.10.0",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-merge": "^4.0.0"
  }
}
