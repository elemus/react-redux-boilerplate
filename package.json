{
  "name": "react-redux-boilerplate",
  "version": "0.0.0",
  "description": "React-Redux Boilerplate",
  "author": "Sergey Atyagin",
  "license": "MIT",
  "homepage": "https://github.com/elemus/react-redux-boilerplate",
  "keywords": [
    "react",
    "react-router",
    "redux",
    "webpack",
    "hot",
    "reload",
    "hmr",
    "boilerplate"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:elemus/react-redux-todo-example.git"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack -p --config ./webpack/config.prod.js --progress --profile --colors --bail",
    "clean": "rimraf dist coverage",
    "coverage": "nyc npm test",
    "lint": "eslint src && sass-lint src/styles/**/*.scss -v -q",
    "start": "webpack-dev-server --config ./webpack/config.dev.js --hot",
    "test": "cross-env NODE_ENV=test mocha --opts ./mocha.opts --recursive"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "include": [
      "src/**/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "check-coverage": true,
    "cache": true
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.0.0-beta",
    "classnames": "^2.2.5",
    "history": "^4.7.2",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.9",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.23.0",
    "chai": "^4.1.2",
    "chai-enzyme": "^0.8.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "jsdom": "^11.3.0",
    "mocha": "^4.0.1",
    "node-sass": "^4.5.3",
    "nyc": "^11.2.1",
    "postcss-loader": "^2.0.6",
    "proxyquire": "^1.8.0",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-test-renderer": "^16.0.0",
    "rimraf": "^2.6.2",
    "sass-lint": "^1.12.0",
    "sass-loader": "^6.0.6",
    "sinon": "^4.0.1",
    "sinon-chai": "^2.14.0",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.0"
  }
}
