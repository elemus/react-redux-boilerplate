{
  "name": "react-redux-boilerplate",
  "version": "0.0.0",
  "description": "React-Redux Boilerplate",
  "author": "Serhii Atiahin",
  "license": "MIT",
  "homepage": "https://github.com/elemus/react-redux-boilerplate",
  "keywords": [
    "react",
    "react-router",
    "redux",
    "webpack",
    "hot",
    "reload",
    "hmr",
    "boilerplate"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:elemus/react-redux-todo-example.git"
  },
  "scripts": {
    "build": "webpack -p --config ./webpack/config.prod.js --progress --profile --colors --bail",
    "clean": "rimraf dist coverage",
    "coverage": "nyc npm test",
    "lint": "eslint src && sass-lint src/styles/**/*.scss -v -q",
    "start": "webpack-dev-server --config ./webpack/config.dev.js --hot",
    "test": "cross-env NODE_ENV=test mocha --opts ./mocha.opts --recursive"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "include": [
      "src/**/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "check-coverage": true,
    "cache": true
  },
  "dependencies": {
    "babel-polyfill": "^7.0.0-beta.3",
    "bootstrap": "^4.1.3",
    "classnames": "^2.2.6",
    "history": "^4.7.2",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.1",
    "autoprefixer": "^9.1.0",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-transform-object-assign": "^7.0.0-beta.3",
    "babel-plugin-transform-react-constant-elements": "^7.0.0-beta.3",
    "babel-plugin-transform-react-inline-elements": "^7.0.0-beta.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.14",
    "babel-plugin-transform-runtime": "^7.0.0-beta.3",
    "babel-preset-env": "^7.0.0-beta.3",
    "babel-preset-react": "^7.0.0-beta.3",
    "babel-register": "^7.0.0-beta.3",
    "chai": "^4.1.2",
    "chai-enzyme": "^1.0.0-beta.1",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "enzyme": "^3.4.1",
    "enzyme-adapter-react-16": "^1.2.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^4.0.0-alpha",
    "jsdom": "^11.12.0",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.2",
    "nyc": "^13.0.1",
    "postcss-loader": "^3.0.0",
    "proxyquire": "^2.0.1",
    "react-addons-test-utils": "^16.0.0-alpha.3",
    "react-hot-loader": "^4.3.4",
    "react-test-renderer": "^16.4.2",
    "rimraf": "^2.6.2",
    "sass-lint": "^1.12.1",
    "sass-loader": "^7.1.0",
    "sinon": "^6.1.5",
    "sinon-chai": "^3.2.0",
    "style-loader": "^0.22.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.4",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.4"
  }
}
