{
  "name": "react-redux-boilerplate",
  "version": "0.0.0",
  "description": "React-Redux Boilerplate",
  "author": "Serhii Atiahin",
  "license": "MIT",
  "homepage": "https://github.com/elemus/react-redux-boilerplate",
  "keywords": [
    "react",
    "react-router",
    "redux",
    "webpack",
    "hot",
    "reload",
    "hmr",
    "boilerplate"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:elemus/react-redux-boilerplate.git"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config ./webpack/config.prod.js --progress --color",
    "clean": "rimraf dist coverage",
    "coverage": "nyc npm run test",
    "lint": "eslint src && sass-lint src/styles/**/*.scss -v -q",
    "start": "webpack serve --config ./webpack/config.dev.js --hot",
    "test": "cross-env NODE_ENV=test mocha -r @babel/register -r ./test/config.js --reporter spec --recursive"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "include": [
      "src/**/*.js"
    ],
    "require": [
      "@babel/register"
    ],
    "check-coverage": true,
    "cache": true
  },
  "dependencies": {
    "bootstrap": "^4.5.3",
    "classnames": "^2.2.6",
    "history": "^5.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-hot-loader": "^4.13.0",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-object-assign": "^7.12.1",
    "@babel/plugin-transform-react-constant-elements": "^7.12.1",
    "@babel/plugin-transform-react-inline-elements": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/register": "^7.12.10",
    "@hot-loader/react-dom": "^16.14.0",
    "autoprefixer": "^10.2.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "chai": "^4.2.0",
    "chai-enzyme": "^1.0.0-beta.1",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.17.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.1",
    "jsdom": "^16.4.0",
    "mini-css-extract-plugin": "^1.3.5",
    "mocha": "^8.2.1",
    "node-sass": "^5.0.0",
    "nyc": "^15.1.0",
    "postcss": "^8.2.7",
    "postcss-loader": "^4.1.0",
    "proxyquire": "^2.1.3",
    "react-test-renderer": "^16.14.0",
    "rimraf": "^3.0.2",
    "sass-lint": "^1.13.1",
    "sass-loader": "^10.0.3",
    "sinon": "^9.2.4",
    "sinon-chai": "^3.5.0",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.1",
    "webpack-merge": "^5.7.3"
  },
  "browserslist": [
    "last 2 versions",
    "not dead"
  ]
}
