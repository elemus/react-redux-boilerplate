{
  "name": "react-redux-boilerplate",
  "version": "0.0.0",
  "description": "React-Redux Boilerplate",
  "author": "Serhii Atiahin",
  "license": "MIT",
  "homepage": "https://github.com/elemus/react-redux-boilerplate",
  "keywords": [
    "react",
    "react-router",
    "redux",
    "webpack",
    "hot",
    "reload",
    "hmr",
    "boilerplate"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:elemus/react-redux-boilerplate.git"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack -p --config ./webpack/config.prod.js --progress --profile --colors --bail",
    "clean": "rimraf dist coverage",
    "coverage": "nyc npm run test",
    "lint": "eslint src && sass-lint src/styles/**/*.scss -v -q",
    "start": "webpack-dev-server --config ./webpack/config.dev.js --hot",
    "test": "cross-env NODE_ENV=test mocha -r @babel/register -r ./test/config.js --reporter spec --recursive"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "include": [
      "src/**/*.js"
    ],
    "require": [
      "@babel/register"
    ],
    "check-coverage": true,
    "cache": true
  },
  "dependencies": {
    "bootstrap": "^4.5.0",
    "classnames": "^2.2.6",
    "history": "^5.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "react-redux": "^7.2.0",
    "react-router": "^5.2.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-transform-object-assign": "^7.10.4",
    "@babel/plugin-transform-react-constant-elements": "^7.10.4",
    "@babel/plugin-transform-react-inline-elements": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@hot-loader/react-dom": "^16.13.0",
    "autoprefixer": "^9.8.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "chai": "^4.2.0",
    "chai-enzyme": "^1.0.0-beta.1",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.6.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "jsdom": "^16.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^8.0.1",
    "node-sass": "^4.14.1",
    "nyc": "^15.1.0",
    "postcss-loader": "^3.0.0",
    "proxyquire": "^2.1.3",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "sass-lint": "^1.13.1",
    "sass-loader": "^9.0.2",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "style-loader": "^1.2.1",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "browserslist": [
    "last 2 versions",
    "not dead"
  ]
}
